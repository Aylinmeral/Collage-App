<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>AgeMustGreaterThan16</fullName>
    <active>true</active>
    <errorConditionFormula>YEAR( TODAY() )-YEAR(Date_of_Birth__c  ) &lt; 16</errorConditionFormula>
    <errorDisplayField>Date_of_Birth__c</errorDisplayField>
    <errorMessage>Your age must be greater than 16</errorMessage>
</ValidationRule>
